{
  "ConnectionStrings": {
    "DefaultConnection": "User ID={POSTGRES_USER};Password={POSTGRES_PASSWORD};Server={POSTGRES_SERVER};Port={POSTGRES_PORT};Database={POSTGRES_DB};"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JWT": {
    "Issuer": "{ISSUER}",
    "Audience": "{AUDIENCE}",
    "SigningKey": "{SIGNING_KEY}"
  },
  "FMPKey": "{FMPKey}"
}
